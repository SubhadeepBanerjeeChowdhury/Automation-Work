import openpyxl

def remove_strikethrough_effect(file_path):
    # Load the workbook
    wb = openpyxl.load_workbook(file_path)
    
    # Iterate through each sheet in the workbook
    for sheet in wb.sheetnames:
        ws = wb[sheet]
        
        # Iterate through each row in the sheet
        for row in ws.iter_rows():
            # Iterate through each cell in the row
            for cell in row:
                # Check if the cell contains any text
                if cell.value and isinstance(cell.value, str):
                    # Check if the cell has a strikethrough effect
                    if cell.font.strike:
                        # Remove the strikethrough text
                        cell.value = cell.value.replace(cell.value, '')
    
    # Save the modified workbook
    wb.save(file_path)

# Example usage
remove_strikethrough_effect('example.xlsx')

