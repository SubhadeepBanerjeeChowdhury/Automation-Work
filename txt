def clean_ded_and_oop(value):
    # Convert the value to a string
    value_str = str(value).lower()
    
    # Find and replace 'd(+)k' with 'd000'
    value_str = re.sub(r'(\d+)k', lambda m: f"{m.group(1)}000", value_str)
    
    # Remove all non-numeric characters
    clean_value = re.sub(r'[^0-9]', '', value_str)
    
    # Return the cleaned numeric value or '' if the result is empty
    return int(clean_value) if clean_value else ''
