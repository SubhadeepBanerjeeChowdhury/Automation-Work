import xlwings as xw

def remove_strikethrough_text(file_path):
    # Open the workbook
    app = xw.App(visible=False)
    workbook = app.books.open(file_path)
    
    for sheet in workbook.sheets:
        for cell in sheet.used_range:
            if cell.value is not None:
                if isinstance(cell.value, str):
                    # Create a new string without strikethrough characters
                    new_value = []
                    for char_index in range(len(cell.value)):
                        # Check if the specific character is strikethrough
                        if not cell.fonts[char_index].strikethrough:
                            new_value.append(cell.value[char_index])
                    cell.value = ''.join(new_value)
    
    # Save and close the workbook
    workbook.save(file_path)
    workbook.close()
    app.quit()

# Usage
file_path = 'your_excel_file.xlsx'
remove_strikethrough_text(file_path)
