from openpyxl import load_workbook

def remove_strikethrough(filename):
    wb = load_workbook(filename)
    for sheet in wb.sheetnames:
        ws = wb[sheet]
        for row in ws.iter_rows():
            for cell in row:
                if cell.font.strike:
                    text = cell.value
                    new_text = ""
                    for char, font in zip(text, cell.fonts):
                        if font.strike:
                            continue
                        new_text += char
                    cell.value = new_text
    wb.save(filename)

# Example usage:
remove_strikethrough('your_excel_file.xlsx')
